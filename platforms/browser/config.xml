<?xml version='1.0' encoding='utf-8'?>
<widget id="kane.eggerz" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Eggerz</name>
    <description>
        A simple clicker game UP834734
    </description>
    <author email="up834734@myport.ac.uk" href="riotz.co.uk">
        UP834734
    </author>
    <content src="/index.html" />
    <access origin="*" />
    <icon src="icon.png" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="26" />
    <preference name="android-versionCode" value="100091" />
    <preference name="fullscreen" value="true" />
    <preference name="orientation" value="portrait" />
</widget>
